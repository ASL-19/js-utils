{
  "name": "@asl-19/js-utils",
  "version": "0.2.0",
  "description": "Shared JavaScript utility functions for ASL19 projects",
  "author": {
    "name": "Grant Heaslip",
    "email": "grant@asl19.org",
    "url": "https://asl19.org/"
  },
  "license": "GPL-3.0-or-later",
  "repository": "ASL-19/js-utils",
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "dist/**/*.js",
    "types/**/*.d.ts"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/js-utils.public.d.ts",
  "sideEffects": false,
  "scripts": {
    "api-extractor:check": "api-extractor run --verbose",
    "api-extractor:update": "api-extractor run --local --verbose",
    "api-documenter:update": "api-documenter markdown --input-folder ./api-extractor --output-folder ./docs",
    "build": "tsc -p tsconfig-cjs.json && tsc -p tsconfig-esm.json",
    "build-and-update-api": "yarn build && yarn api-extractor:update && yarn api-documenter:update",
    "eslint": "eslint . --ext js,ts",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "@asl-19/eslint-config-typescript": "^0.4.4",
    "@microsoft/api-documenter": "^7.8.8",
    "@microsoft/api-extractor": "^7.8.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sort-destructure-keys": "^1.3.4",
    "eslint-plugin-tsdoc": "^0.2.5",
    "eslint-plugin-unicorn": "^18.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  }
}
