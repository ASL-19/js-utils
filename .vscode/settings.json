{
  "cSpell.words": [
    "Perso",
    "destructure",
    "heaslip",
    "rushstack",
    "tsdoc",
    "unscoped"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "typescript"
  ],
  "files.associations": {
    "api-extractor.json": "jsonc"
  },
  "search.exclude": {
    ".yarn/releases/**/*": true,
    ".yarnrc": true,
    "api-extractor/**/*": true,
    "CHANGELOG": true,
    "docs/**/*": true,
    "LICENSE": true,
    "types/**/*": true,
    "yarn.lock": true
  }
}
